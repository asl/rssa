useDynLib(Rssa)

import(lattice)
import(svd)
import(forecast)

export(clone,
       decompose,
       decompose.default,
       reconstruct,
       nu,
       nv,
       nlambda,
       calc.v,
       precache,
       cleanup,
       new.ssa,
       ssa,
       clusterify,
       wcor,
       wcor.default,
       hmatr,
# Hankel matrix' routines
       new.hmat,
       hmatmul,
       hankel,
       hcols,
       hrows,
       is.hmat,
# Hankel-block hankel matrix' routines
       new.hbhmat,
       hbhmatmul,
       hbhcols,
       hbhrows,
       is.hbhmat,
# Symmetric toeplitz matrix' routines
       new.tmat,
       tmatmul,
       tcols,
       trows,
       is.tmat,
# Forecast stuff
       lrr,
       roots,
       rforecast,
       vforecast,
       bforecast,
# Period estimation
       parestimate
       )

S3method("clone", ssa)
